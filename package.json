{
  "name": "combined-playlists",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "build": "spicetify-creator",
    "build:local": "spicetify-creator --out=dist/combined-playlists --minify",
    "build:prod": "npm run build:local && npm run copy:docs",
    "copy:docs": "copyfiles README.md docs/* dist/combined-playlists",
    "watch": "spicetify-creator --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@jeroentvb/eslint-config-typescript": "^1.3.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/spotify-api": "^0.0.25",
    "copyfiles": "^2.4.1",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "spicetify-creator": "^1.0.17"
  },
  "dependencies": {
    "formik": "^2.4.6",
    "semver": "^7.6.3"
  }
}
